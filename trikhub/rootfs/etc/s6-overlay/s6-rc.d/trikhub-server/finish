#!/command/with-contenv bashio
# ==============================================================================
# TrikHub Server - Finish Script
# ==============================================================================

bashio::log.info "TrikHub Server stopped"

# If the service exits with a non-zero code, log it
if [ "${1}" -ne 0 ] && [ "${1}" -ne 256 ]; then
    bashio::log.warning "TrikHub Server exited with code ${1}"
fi
